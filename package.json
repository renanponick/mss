  "scripts": {
    "build": "tsc",
    "db": "cd migrations && ../scripts/sqitch.sh",
    "db:clean": "echo 'DROP SCHEMA IF EXISTS public, sqitch CASCADE; CREATE SCHEMA public' | psql postgres://postgres:123456@localhost/postgres",
    "test": "cross-env NODE_ENV=test mocha --timeout 10000 --exit -r ts-node/register/transpile-only test/entities/**",
    "lint": "eslint  --ext .ts --cache src test",
    "type-check": "tsc --noEmit",
    "db:seed": "cd seeds && ../scripts/seed.ts",
    "db:check": "OMIT_DB_LOGS=true ts-node --transpile-only scripts/check-database.ts",
    "dev:down": "docker-compose down",
    "dev:up": "docker-compose up -d",
    "dev:init": "run-s dev:down dev:up",
    "test:setup": "yarn -s db:clean && yarn -s db deploy && yarn -s db:seed apply default",
    "dev:watch": "ts-node-dev --respawn --transpileOnly src/server.ts",
    "start": "node build/src/server.js"
  }
}
